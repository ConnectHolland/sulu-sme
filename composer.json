{
    "name": "connectholland/sulu-sme",
    "license": "proprietary",
    "type": "project",
    "description": "Base for Sulu small and medium sized enterprises",
    "version": "dev-master",
    "autoload": {
        "psr-0": {
            "": "src/"
        },
        "classmap": [
            "app/AbstractKernel.php",
            "app/AdminKernel.php",
            "app/WebsiteCache.php",
            "app/WebsiteKernel.php"
        ]
    },
    "require": {
        "php": ">=5.5",
        "connectholland/sulu-admin-bundle": "^1.1",
        "connectholland/sulu-block-bundle": "dev-master",
        "dantleech/phpcr-migrations-bundle": "0.1.*",
        "doctrine/doctrine-bundle": "^1.0",
        "doctrine/doctrine-cache-bundle": "^1.0",
        "doctrine/doctrine-fixtures-bundle": "2.2.*",
        "doctrine/doctrine-migrations-bundle": "^1.2",
        "incenteev/composer-parameter-handler": "^2.0",
        "jackalope/jackalope-doctrine-dbal": "^1.2.5",
        "jackalope/jackalope-jackrabbit": "^1.2.0",
        "massive/build-bundle": "0.2.*",
        "oro/doctrine-extensions": "^1.0",
        "sensio/distribution-bundle": "^5.0",
        "sensio/framework-extra-bundle": "^3.0.2",
        "sentry/sentry": "^1.6",
        "sulu/sulu": "1.5.*",
        "symfony/monolog-bundle": "^2.4",
        "twig/extensions": "^1.0",
        "zendframework/zend-stdlib": "^2.3",
        "zendframework/zendsearch": "@dev"
    },
    "require-dev": {
        "phpcr/phpcr-shell": "@beta",
        "sensio/generator-bundle": "^3.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": {
        "connectholland": {
            "type": "composer",
            "url": "https://composer.packages.connectholland.nl/"
        }
    },
    "scripts": {
    },
    "config": {
        "bin-dir": "bin",
        "sort-packages": true
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-web-dir": "web",
        "symfony-var-dir": "var",
        "symfony-assets-install": "relative"
    }
}
